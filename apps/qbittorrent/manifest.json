{
  "id": "qbittorrent",
  "name": "qBittorrent",
  "version": "5.1.2-auth-proxy-patch",
  "description": {
    "en": "Free and reliable P2P Bittorrent client",
    "fr": "Client BitTorrent puissant avec interface web"
  },
  "author": "qBittorrent",
  "license": null,
  "website": "https://qbittorrent.org/",
  "icon": "mdi:download-circle",
  "requirements": {
    "min_ram": 256,
    "min_disk": 200,
    "arch": [
      "aarch64",
      "x86_64"
    ],
    "dependencies": [
      "docker"
    ]
  },
  "install": {
    "type": "docker",
    "image": "ghcr.io/hotio/qbittorrent:release-5.1.2",
    "container": {
      "name": "qbittorrent",
      "hostname": "qbittorrent",
      "image": "ghcr.io/hotio/qbittorrent:release-5.1.2",
      "restart": "on-failure",
      "ports": [
        {
          "host": 8094,
          "container": 8080,
          "protocol": "tcp"
        }
      ],
      "volumes": [
        {
          "host": "${APP_DATA_DIR}/data/config",
          "container": "/config",
          "readonly": false
        },
        {
          "host": "${DATA_DIR}/storage/downloads",
          "container": "/app/qBittorrent/downloads",
          "readonly": false
        }
      ],
      "environment": [
        {
          "name": "PUID",
          "value": "1000"
        },
        {
          "name": "PGID",
          "value": "1000"
        }
      ],
      "labels": {
        "com.pinas.managed": "true",
        "com.pinas.app": "qbittorrent"
      }
    },
    "steps": [
      {
        "action": "mkdir",
        "path": "${APP_DATA_DIR}/data/config"
      },
      {
        "action": "mkdir",
        "path": "${DATA_DIR}/storage/downloads"
      },
      {
        "action": "docker_pull",
        "image": "ghcr.io/hotio/qbittorrent:release-5.1.2"
      },
      {
        "action": "docker_create",
        "config": {
          "name": "qbittorrent",
          "hostname": "qbittorrent",
          "image": "ghcr.io/hotio/qbittorrent:release-5.1.2",
          "restart": "on-failure",
          "ports": [
        {
          "host": 8094,
          "container": 8080,
          "protocol": "tcp"
        }
      ],
          "volumes": [
            {
              "host": "${APP_DATA_DIR}/data/config",
              "container": "/config",
              "readonly": false
            },
            {
              "host": "${DATA_DIR}/storage/downloads",
              "container": "/app/qBittorrent/downloads",
              "readonly": false
            }
          ],
          "environment": [
            {
              "name": "PUID",
              "value": "1000"
            },
            {
              "name": "PGID",
              "value": "1000"
            }
          ],
          "labels": {
            "com.pinas.managed": "true",
            "com.pinas.app": "qbittorrent"
          }
        }
      },
      {
        "action": "docker_start",
        "container": "qbittorrent"
      }
    ]
  },
  "uninstall": {
    "steps": [
      {
        "action": "docker_stop",
        "container": "qbittorrent"
      },
      {
        "action": "docker_rm",
        "container": "qbittorrent"
      }
    ]
  },
  "files": {},
  "config": {},
  "frontend": {
    "component": "WebviewApp",
    "icon": "mdi:download-circle",
    "gradient": "from-blue-600 to-blue-800",
    "window": {
      "width": 1200,
      "height": 800,
      "min_width": 900,
      "min_height": 600
    },
    "config": {
      "port": 8094,
      "path": "",
      "title": "qBittorrent"
    },
    "i18n": {
      "en": {
        "description": "Free and reliable P2P Bittorrent client"
      },
      "fr": {
        "description": "Client BitTorrent puissant avec interface web"
      }
    }
  }
}
