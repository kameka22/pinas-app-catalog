{
  "id": "emby",
  "name": "Emby",
  "version": "4.9.3.0",
  "description": {
    "en": "A personal media server",
    "fr": "Serveur multimédia personnel"
  },
  "author": "Emby LLC",
  "license": null,
  "website": "https://emby.media/",
  "icon": "mdi:play-network",
  "requirements": {
    "min_ram": 256,
    "min_disk": 200,
    "arch": [
      "aarch64",
      "x86_64"
    ],
    "dependencies": [
      "docker"
    ]
  },
  "install": {
    "type": "docker",
    "image": "linuxserver/emby:version-4.9.3.0",
    "container": {
      "name": "emby",
      "hostname": "emby",
      "image": "linuxserver/emby:version-4.9.3.0",
      "restart": "on-failure",
      "ports": [
        {
          "host": 8097,
          "container": 8096,
          "protocol": "tcp"
        }
      ],
      "volumes": [
        {
          "host": "${APP_DATA_DIR}/data/config",
          "container": "/config",
          "readonly": false
        },
        {
          "host": "${DATA_DIR}/storage/downloads",
          "container": "/downloads",
          "readonly": false
        }
      ],
      "environment": [
        {
          "name": "TZ",
          "value": "Europe/Berlin"
        },
        {
          "name": "UID",
          "value": "1000"
        },
        {
          "name": "GID",
          "value": "1000"
        }
      ],
      "labels": {
        "com.pinas.managed": "true",
        "com.pinas.app": "emby"
      }
    },
    "steps": [
      {
        "action": "mkdir",
        "path": "${APP_DATA_DIR}/data/config"
      },
      {
        "action": "mkdir",
        "path": "${DATA_DIR}/storage/downloads"
      },
      {
        "action": "docker_pull",
        "image": "linuxserver/emby:version-4.9.3.0"
      },
      {
        "action": "docker_create",
        "config": {
          "name": "emby",
          "hostname": "emby",
          "image": "linuxserver/emby:version-4.9.3.0",
          "restart": "on-failure",
          "ports": [
        {
          "host": 8097,
          "container": 8096,
          "protocol": "tcp"
        }
      ],
          "volumes": [
            {
              "host": "${APP_DATA_DIR}/data/config",
              "container": "/config",
              "readonly": false
            },
            {
              "host": "${DATA_DIR}/storage/downloads",
              "container": "/downloads",
              "readonly": false
            }
          ],
          "environment": [
            {
              "name": "TZ",
              "value": "Europe/Berlin"
            },
            {
              "name": "UID",
              "value": "1000"
            },
            {
              "name": "GID",
              "value": "1000"
            }
          ],
          "labels": {
            "com.pinas.managed": "true",
            "com.pinas.app": "emby"
          }
        }
      },
      {
        "action": "docker_start",
        "container": "emby"
      }
    ]
  },
  "uninstall": {
    "steps": [
      {
        "action": "docker_stop",
        "container": "emby"
      },
      {
        "action": "docker_rm",
        "container": "emby"
      }
    ]
  },
  "files": {},
  "config": {},
  "frontend": {
    "component": "WebviewApp",
    "icon": "mdi:play-network",
    "gradient": "from-green-500 to-emerald-600",
    "window": {
      "width": 1200,
      "height": 800,
      "min_width": 900,
      "min_height": 600
    },
    "config": {
      "port": 8097,
      "path": "",
      "title": "Emby"
    },
    "i18n": {
      "en": {
        "description": "A personal media server"
      },
      "fr": {
        "description": "Serveur multimédia personnel"
      }
    }
  }
}
