{
  "id": "plex",
  "name": "Plex",
  "version": "1.43.0.10467",
  "description": {
    "en": "Stream Movies & TV Shows",
    "fr": "Serveur multimédia pour diffuser vos films, séries et musique"
  },
  "author": "Plex",
  "license": null,
  "website": "https://www.plex.tv/",
  "icon": "mdi:plex",
  "requirements": {
    "min_ram": 256,
    "min_disk": 200,
    "arch": [
      "aarch64",
      "x86_64"
    ],
    "dependencies": [
      "docker"
    ]
  },
  "install": {
    "type": "docker",
    "image": "linuxserver/plex:1.43.0",
    "container": {
      "name": "plex",
      "hostname": "plex",
      "image": "linuxserver/plex:1.43.0",
      "restart": "on-failure",
      "ports": [],
      "volumes": [
        {
          "host": "${APP_DATA_DIR}/data/config",
          "container": "/config",
          "readonly": false
        },
        {
          "host": "${APP_DATA_DIR}/data/transcode",
          "container": "/transcode",
          "readonly": false
        },
        {
          "host": "${DATA_DIR}/storage/downloads",
          "container": "/downloads",
          "readonly": false
        }
      ],
      "environment": [
        {
          "name": "PUID",
          "value": "1000"
        },
        {
          "name": "PGID",
          "value": "1000"
        },
        {
          "name": "VERSION",
          "value": "docker"
        }
      ],
      "labels": {
        "com.pinas.managed": "true",
        "com.pinas.app": "plex"
      },
      "network": "host"
    },
    "steps": [
      {
        "action": "mkdir",
        "path": "${APP_DATA_DIR}/data/config"
      },
      {
        "action": "mkdir",
        "path": "${APP_DATA_DIR}/data/transcode"
      },
      {
        "action": "mkdir",
        "path": "${DATA_DIR}/storage/downloads"
      },
      {
        "action": "docker_pull",
        "image": "linuxserver/plex:1.43.0"
      },
      {
        "action": "docker_create",
        "config": {
          "name": "plex",
          "hostname": "plex",
          "image": "linuxserver/plex:1.43.0",
          "restart": "on-failure",
          "ports": [],
          "volumes": [
            {
              "host": "${APP_DATA_DIR}/data/config",
              "container": "/config",
              "readonly": false
            },
            {
              "host": "${APP_DATA_DIR}/data/transcode",
              "container": "/transcode",
              "readonly": false
            },
            {
              "host": "${DATA_DIR}/storage/downloads",
              "container": "/downloads",
              "readonly": false
            }
          ],
          "environment": [
            {
              "name": "PUID",
              "value": "1000"
            },
            {
              "name": "PGID",
              "value": "1000"
            },
            {
              "name": "VERSION",
              "value": "docker"
            }
          ],
          "labels": {
            "com.pinas.managed": "true",
            "com.pinas.app": "plex"
          },
          "network": "host"
        }
      },
      {
        "action": "docker_start",
        "container": "plex"
      }
    ]
  },
  "uninstall": {
    "steps": [
      {
        "action": "docker_stop",
        "container": "plex"
      },
      {
        "action": "docker_rm",
        "container": "plex"
      }
    ]
  },
  "files": {},
  "config": {},
  "frontend": {
    "component": "WebviewApp",
    "icon": "mdi:plex",
    "gradient": "from-amber-500 to-yellow-600",
    "window": {
      "width": 1200,
      "height": 800,
      "min_width": 900,
      "min_height": 600
    },
    "config": {
      "port": 32400,
      "path": "/web",
      "title": "Plex"
    },
    "i18n": {
      "en": {
        "description": "Stream Movies & TV Shows"
      },
      "fr": {
        "description": "Serveur multimédia pour diffuser vos films, séries et musique"
      }
    }
  }
}
