{
  "id": "jellyfin",
  "name": "Jellyfin",
  "version": "10.11.6",
  "description": {
    "en": "The Free Software Media System",
    "fr": "Serveur multimédia libre pour diffuser vos films, séries et musique"
  },
  "author": "Jellyfin",
  "license": null,
  "website": "https://jellyfin.org/",
  "icon": "mdi:filmstrip",
  "requirements": {
    "min_ram": 256,
    "min_disk": 200,
    "arch": [
      "aarch64",
      "x86_64"
    ],
    "dependencies": [
      "docker"
    ]
  },
  "install": {
    "type": "docker",
    "image": "linuxserver/jellyfin:10.11.6",
    "container": {
      "name": "jellyfin",
      "hostname": "jellyfin",
      "image": "linuxserver/jellyfin:10.11.6",
      "restart": "on-failure",
      "ports": [
        {
          "host": 8096,
          "container": 8096,
          "protocol": "tcp"
        },
        {
          "host": 7359,
          "container": 7359,
          "protocol": "udp"
        }
      ],
      "volumes": [
        {
          "host": "${APP_DATA_DIR}/data/config",
          "container": "/config",
          "readonly": false
        },
        {
          "host": "${DATA_DIR}/storage/downloads",
          "container": "/downloads",
          "readonly": false
        }
      ],
      "environment": [
        {
          "name": "PUID",
          "value": "1000"
        },
        {
          "name": "PGID",
          "value": "1000"
        }
      ],
      "labels": {
        "com.pinas.managed": "true",
        "com.pinas.app": "jellyfin"
      }
    },
    "steps": [
      {
        "action": "mkdir",
        "path": "${APP_DATA_DIR}/data/config"
      },
      {
        "action": "mkdir",
        "path": "${DATA_DIR}/storage/downloads"
      },
      {
        "action": "docker_pull",
        "image": "linuxserver/jellyfin:10.11.6"
      },
      {
        "action": "docker_create",
        "config": {
          "name": "jellyfin",
          "hostname": "jellyfin",
          "image": "linuxserver/jellyfin:10.11.6",
          "restart": "on-failure",
          "ports": [
            {
              "host": 8096,
              "container": 8096,
              "protocol": "tcp"
            },
            {
              "host": 7359,
              "container": 7359,
              "protocol": "udp"
            }
          ],
          "volumes": [
            {
              "host": "${APP_DATA_DIR}/data/config",
              "container": "/config",
              "readonly": false
            },
            {
              "host": "${DATA_DIR}/storage/downloads",
              "container": "/downloads",
              "readonly": false
            }
          ],
          "environment": [
            {
              "name": "PUID",
              "value": "1000"
            },
            {
              "name": "PGID",
              "value": "1000"
            }
          ],
          "labels": {
            "com.pinas.managed": "true",
            "com.pinas.app": "jellyfin"
          }
        }
      },
      {
        "action": "docker_start",
        "container": "jellyfin"
      }
    ]
  },
  "uninstall": {
    "steps": [
      {
        "action": "docker_stop",
        "container": "jellyfin"
      },
      {
        "action": "docker_rm",
        "container": "jellyfin"
      }
    ]
  },
  "files": {},
  "config": {},
  "frontend": {
    "component": "WebviewApp",
    "icon": "mdi:filmstrip",
    "gradient": "from-purple-500 to-indigo-600",
    "window": {
      "width": 1200,
      "height": 800,
      "min_width": 900,
      "min_height": 600
    },
    "config": {
      "port": 8096,
      "path": "",
      "title": "Jellyfin"
    },
    "i18n": {
      "en": {
        "description": "The Free Software Media System"
      },
      "fr": {
        "description": "Serveur multimédia libre pour diffuser vos films, séries et musique"
      }
    }
  }
}
