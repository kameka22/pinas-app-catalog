{
  "id": "sabnzbd",
  "name": "SABnzbd",
  "version": "4.5.5",
  "description": {
    "en": "The automated Usenet download tool",
    "fr": "Téléchargeur Usenet automatisé"
  },
  "author": "sabnzbd",
  "license": null,
  "website": "https://sabnzbd.org/",
  "icon": "mdi:download-box",
  "requirements": {
    "min_ram": 256,
    "min_disk": 200,
    "arch": [
      "aarch64",
      "x86_64"
    ],
    "dependencies": [
      "docker"
    ]
  },
  "install": {
    "type": "docker",
    "image": "linuxserver/sabnzbd:4.5.5",
    "container": {
      "name": "sabnzbd",
      "hostname": "sabnzbd",
      "image": "linuxserver/sabnzbd:4.5.5",
      "restart": "on-failure",
      "ports": [],
      "volumes": [
        {
          "host": "${APP_DATA_DIR}/data/config",
          "container": "/config",
          "readonly": false
        },
        {
          "host": "${DATA_DIR}/storage/downloads",
          "container": "/downloads",
          "readonly": false
        }
      ],
      "environment": [
        {
          "name": "DOCKER_MODS",
          "value": "linuxserver/mods:universal-package-install"
        },
        {
          "name": "INSTALL_PACKAGES",
          "value": "ffmpeg"
        },
        {
          "name": "PUID",
          "value": "1000"
        },
        {
          "name": "PGID",
          "value": "1000"
        },
        {
          "name": "TZ",
          "value": "Etc/UTC"
        }
      ],
      "labels": {
        "com.pinas.managed": "true",
        "com.pinas.app": "sabnzbd"
      }
    },
    "steps": [
      {
        "action": "mkdir",
        "path": "${APP_DATA_DIR}/data/config"
      },
      {
        "action": "mkdir",
        "path": "${DATA_DIR}/storage/downloads"
      },
      {
        "action": "docker_pull",
        "image": "linuxserver/sabnzbd:4.5.5"
      },
      {
        "action": "docker_create",
        "config": {
          "name": "sabnzbd",
          "hostname": "sabnzbd",
          "image": "linuxserver/sabnzbd:4.5.5",
          "restart": "on-failure",
          "ports": [],
          "volumes": [
            {
              "host": "${APP_DATA_DIR}/data/config",
              "container": "/config",
              "readonly": false
            },
            {
              "host": "${DATA_DIR}/storage/downloads",
              "container": "/downloads",
              "readonly": false
            }
          ],
          "environment": [
            {
              "name": "DOCKER_MODS",
              "value": "linuxserver/mods:universal-package-install"
            },
            {
              "name": "INSTALL_PACKAGES",
              "value": "ffmpeg"
            },
            {
              "name": "PUID",
              "value": "1000"
            },
            {
              "name": "PGID",
              "value": "1000"
            },
            {
              "name": "TZ",
              "value": "Etc/UTC"
            }
          ],
          "labels": {
            "com.pinas.managed": "true",
            "com.pinas.app": "sabnzbd"
          }
        }
      },
      {
        "action": "docker_start",
        "container": "sabnzbd"
      }
    ]
  },
  "uninstall": {
    "steps": [
      {
        "action": "docker_stop",
        "container": "sabnzbd"
      },
      {
        "action": "docker_rm",
        "container": "sabnzbd"
      }
    ]
  },
  "files": {},
  "config": {},
  "frontend": {
    "component": "IframeApp",
    "icon": "mdi:download-box",
    "gradient": "from-yellow-500 to-amber-600",
    "window": {
      "width": 1200,
      "height": 800,
      "min_width": 900,
      "min_height": 600
    },
    "config": {
      "port": 9876,
      "path": "",
      "title": "SABnzbd"
    },
    "i18n": {
      "en": {
        "description": "The automated Usenet download tool"
      },
      "fr": {
        "description": "Téléchargeur Usenet automatisé"
      }
    }
  }
}
